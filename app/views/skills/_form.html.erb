<div class="panel panel-default">
  <div class="panel-heading">
    Create your skill
  </div>
  <div class="panel-body">
    <div class="container">
      <%= simple_form_for @skill do |f| %>
        <div class="row">
          <div class="col-md-4 select">
            <div class="form-group">
              <label>Entrer votre comp√©tence</label>
              <%= f.input :listing_skill, as: :select, collection: Skill::LISTING_SKILL, include_blank:  "Select a category" %>
            </div>
            <%= f.submit class: "btn btn-primary" %>
          </div>
        </div>
      <% end %>
  </div>
</div>
